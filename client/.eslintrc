{
  "parser": "babel-eslint",
  "env": {
  	"browser": true,
  	"es6": true,
  	"node": true
  },
  "parserOptions": {
  	"ecmaVersion": 6,
  	"sourceType": "module"
  },
  "extends": "airbnb",
  "rules": {
		"semi": [0],
		"react/jsx-filename-extension": [0],
		"global-require": [0],
		"space-before-function-paren": [0],
		"indent": [0],
		"no-tabs": [0],
		"react/jsx-indent": [0],
		"jsx-a11y/anchor-is-valid": [0]
  }
}
